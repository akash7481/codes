   "source": [
    "year = int(input(\"enter the year\"))\n",
    "if (year % 4) == 0:\n",
    "   if (year % 100) == 0:\n",
    "       if (year % 400) == 0:\n",
    "           print(\"it is a leap year\")\n",
    "       else:\n",
    "           print(\"it is not a leap year\")\n",
    "   else:\n",
    "       print(\"it is a leap year\")\n",
    "else:\n",
    "   print(\"it is not a leap year\")"
